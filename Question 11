#include <iostream>
#include <cmath>
using namespace std;

void computeSquareRoot(int* num, double* sqr) {
    *sqr = sqrt(*num);
}

int main() {
    int num;
    double sqr;
    cout << "Enter a number: ";
    cin >> num;
    
    computeSquareRoot(&num, &sqr);
    
    cout << "Square Root: " << sqr << endl;
    
    return 0;
}
