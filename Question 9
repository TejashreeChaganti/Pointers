#include <iostream>
using namespace std;

bool isComposite(int* num) {
    if (*num <= 1) return false;
    if (*num == 2 || *num == 3) return false;
    for (int i = 2; i <= *num / 2; i++) {
        if (*num % i == 0) return true;
    }
    return false;
}

int main() {
    int num;
    cout << "Enter a number: ";
    cin >> num;
    
    if (isComposite(&num))
        cout << num << " is a composite number" << endl;
    else
        cout << num << " is not a composite number" << endl;
    
    return 0;
}
